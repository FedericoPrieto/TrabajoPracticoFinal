@model List<RapiTurnosDTO.DTOs.EmpresasServiciosDTO>

@{
    ViewData["Title"] = "EmpresasServiciosAlta";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>

<h1>Empresa: xxx</h1>

<body>
    <div class="container">
        <h2>Alta de Servicios</h2>
        <p>Ingrese datos</p>
        <form>
            <div class="form-group">
                <label for="usr">Descripcion Servicio:</label>
                <input type="text" class="form-control" id="Descripcion">
            </div>
            <div class="form-group">
                <label for="pwd">Precio Sugerido:</label>
                <input type="text" class="form-control" id="Precio">
            </div>
        </form>
    </div>
    <div class="form-group">
        <label for="sel1">Duracion Turno:</label>
        <select class="form-control" id="sel1">
            <option>1 hora</option>
            <option>2 horas</option>
            <option>pepe...</option>
            <option>pepe...</option>
        </select>
    </div>
    <div class="container">
        <input type="submit" class="btn btn-info" value="Grabar" onclick="eatFood()">
    </div>

</body>
</html>



<script type="text/javascript">
    function eatFood() {
        alert('hola' + document.forms[0].elements[0].value);
    }

</script>




