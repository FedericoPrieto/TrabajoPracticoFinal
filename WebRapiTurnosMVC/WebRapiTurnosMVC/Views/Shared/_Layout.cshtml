<body>
    <header>
        <nav class="navbar navbar-inverse">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">www.RapiTurnos.com.ar</a>
                </div>
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">Home</a></li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#"> Empresa <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="https://localhost:44343/Empresas">Datos Empresas</a></li>
                            <li><a href="https://localhost:44343/EmpresasServicios">Servicios</a></li>
                            <li><a href="https://localhost:44343/DuracionTurnos">Duracion Turnos</a></li>
                            <li><a href="">Personas</a></li>
                            <li><a href="">Personas Servicios</a></li>
                        </ul>
                    </li>
                    @*<li><a href="#">Page 2</a></li>*@
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="https://localhost:44343/Empresas/Create"><span class="glyphicon glyphicon-user"></span> Registrarse</a></li>
                    <li><a href="https://localhost:44343/Login"><span class="glyphicon glyphicon-log-in"></span> Ingreso Empresas</a></li>
                    <li><a href="https://localhost:44343/Login/Logout"><span class="glyphicon glyphicon-log-out"></span> Cerrar Sesion</a></li>
                </ul>
            </div>
        </nav>

    </header>
    <div class="container">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

